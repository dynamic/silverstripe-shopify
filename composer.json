{
    "name": "dynamic/silverstripe-shopify",
    "description": "A Shopify Store module for Silverstripe",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "dynamic/silverstripe-site-tools": "^1.1",
        "guzzlehttp/guzzle": "^6.3",
        "littlegiant/silverstripe-catalogmanager": "^5.2",
        "silverstripe/recipe-cms": "^4.5",
        "symbiote/silverstripe-gridfieldextensions": "^3.0",
        "osiset/basic-shopify-api": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0",
        "phpmd/phpmd": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Dynamic\\Shopify\\": "src/",
            "Dynamic\\Shopify\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "expose": [
            "client"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
