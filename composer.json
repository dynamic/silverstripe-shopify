{
    "name": "dynamic/silverstripe-shopify",
    "description": "A Shopify Store module for Silverstripe",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "littlegiant/silverstripe-catalogmanager": "^5.2.0",
        "guzzlehttp/guzzle": "^7.2",
        "silverstripe/recipe-cms": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^3.0",
        "phploc/phploc": "^4.0",
        "pdepend/pdepend": "^2.5",
        "theseer/phpdox": "^0.11"
    },
    "autoload": {
        "psr-4": {
            "Dynamic\\Shopify\\": "src/",
            "Dynamic\\Shopify\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
